{
    "name": "willow-pays-firestore",
    "devDependencies": {
        "@tsconfig/node18": "^18.2.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/lodash": "^4.14.195",
        "@types/ndjson": "^2.0.1",
        "@types/uuid": "^9.0.2",
        "dotenv": "^16.3.1",
        "jest": "^29.6.1",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^6.2.1",
        "@google-cloud/firestore": "^6.6.1",
        "@google-cloud/functions-framework": "^3.3.0",
        "@google-cloud/tasks": "^3.1.2",
        "express": "^4.18.2",
        "joi": "^17.9.2",
        "lodash": "^4.17.21",
        "ndjson": "^2.0.0",
        "uuid": "^9.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "./src/.*.test.ts$",
        "setupFiles": [
            "dotenv/config"
        ],
        "testTimeout": 540000
    },
    "eslintConfig": {
        "env": {
            "es2021": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-explicit-any": 0
        }
    }
}
